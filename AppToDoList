package com.mycompany.apptodolist;
import ucb.aplicativo.model.Tarefa;

public class AppToDoList {

    public static void main(String[] args) {
        Tarefa minhaAtividade = new Tarefa("Fazer a atividade", "Criar um código para a matéria");
        
        System.out.println(minhaAtividade.obterDetalhes());
        
        minhaAtividade.setCompleta(true);
        System.out.println("A tarefa foi concluída? " + (minhaAtividade.estaCompleta() ? "Sim" : "Não"));
    }
}
